@model Student
<div class = row>
    <div class=" card rounded shadow p-4 mt-4">
        <table class="table table-sm">
            <thead>
                <tr>
                    <th>Issue</th>
                    <th>Created</th>
                    <th>Active</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var t in Model.Tickets) {
                    <tr>
                        <td>@t.Issue</td>
                        <td>@t.CreatedOn</td>
                        <td>@t.Active</td>
                      <td> <a asp-controller="Student" asp-action="TicketDelete" asp-route-id="@Model.Id" class="btn btn-sm btn-danger">Delete</a></td>
                    </tr>
                }        
            </tbody>
        </table>
    </div>
</div>